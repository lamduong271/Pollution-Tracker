{
  "expo": {
    "name": "AirPollution",
    "description": "A very interesting project.",
    "slug": "AirPollution",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Nós utilizamos sua localização para filtrar o conteúdo de acordo com sua região ",
        "NSLocationAlwaysUsageDescription": "Nós utilizamos sua localização para filtrar o conteúdo de acordo com sua região "
      }
    }
  }
}
